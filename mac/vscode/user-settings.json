{
    "editor.wordWrap": "on",
    "workbench.editorAssociations": {
      "*.ipynb": "jupyter-notebook",
      "*.wav": "default"
    },
    "notebook.cellToolbarLocation": {
      "default": "right",
      "jupyter-notebook": "left"
    },
    "redhat.telemetry.enabled": true,
    "git.autofetch": true,
    "markdownlint.config": {},
    "editor.fontLigatures": true,
    "editor.fontFamily": "JetBrainsMono Nerd Font",
    "editor.fontSize": 14,
    "docker.images.label": "Tag",
    "yaml.customTags": [
      "!And",
      "!And sequence",
      "!If",
      "!If sequence",
      "!Not",
      "!Not sequence",
      "!Equals",
      "!Equals sequence",
      "!Or",
      "!Or sequence",
      "!FindInMap",
      "!FindInMap sequence",
      "!Base64",
      "!Join",
      "!Join sequence",
      "!Cidr",
      "!Ref",
      "!Sub",
      "!Sub sequence",
      "!GetAtt",
      "!GetAZs",
      "!ImportValue",
      "!ImportValue sequence",
      "!Select",
      "!Select sequence",
      "!Split",
      "!Split sequence"
    ],
    "json.schemas": [],
    "gitlens.advanced.messages": {
      "suppressGitMissingWarning": true
    },
    "files.exclude": {
      "**/.classpath": true,
      "**/.factorypath": true,
      "**/.project": true,
      "**/.settings": true
    },
    "editor.suggest.showMethods": true,
    "editor.suggest.preview": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.snippetSuggestions": "top",
    "workbench.preferredDarkColorTheme": "One Dark Pro Flat",
    "workbench.preferredHighContrastColorTheme": "One Dark Pro Flat",
    "workbench.preferredHighContrastLightColorTheme": "One Dark Pro Flat",
    "workbench.preferredLightColorTheme": "One Dark Pro Flat",
    "workbench.startupEditor": "none",
    "githubPullRequests.pullBranch": "never",
    "editor.matchBrackets": "never",
    "git.timeline.showUncommitted": true,
    "aws.suppressPrompts": {
      "regionAddAutomatically": true,
      "fileViewerEdit": true
    },
    "editor.formatOnSaveMode": "modifications",
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorCustomizations": {
      "editor.lineHighlightBackground": "#1073cf2d",
      "editor.lineHighlightBorder": "#9fced11f"
    },
    "[yaml]": {
      "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.indentation": false,
    "editor.guides.bracketPairs": "active",
    "editor.inlayHints.enabled": false,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "terminal.integrated.fontSize": 14,
    "[python]": {
      "editor.formatOnType": true,
      "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "editor.fontWeight": "normal",
    "[jsonc]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "hediet.vscode-drawio.resizeImages": null,
    "aws.resources.enabledResources": [],
    "[github-actions-workflow]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[dockerfile]": {
      "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "aws.codeWhisperer.importRecommendation": false,
    "aws.codeWhisperer.includeSuggestionsWithCodeReferences": false,
    "terminal.integrated.env.osx": {},
    "aws.samcli.location": "/opt/homebrew/bin/sam",
    "editor.accessibilitySupport": "off",
    "workbench.colorTheme": "One Dark Pro Flat",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "screencastMode.fontSize": 48,
    "screencastMode.verticalOffset": 12,
    "editor.minimap.autohide": true,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderLineHighlight": "gutter",
    "apc.electron": {
      "frame": false,
    },
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "githubPullRequests.queries": [
      {
        "label": "Waiting For My Review",
        "query": "is:open review-requested:${user}"
      },
      {
        "label": "Assigned To Me",
        "query": "is:open assignee:${user}"
      },
      {
        "label": "Created By Me",
        "query": "is:open author:${user}"
      }
    ],
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "cSpell.userWords": [
      "boto"
    ],
    "explorer.confirmDelete": false,
    "editor.mouseWheelZoom": true
  }
  